{
    "compilerOptions": {
        "target": "es2023",
        "module": "es2022",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "allowJs": true,
        "strict": true,
        "strictNullChecks": false,
        "strictPropertyInitialization": false,
        "rootDir": "webapp",
        "baseUrl": "./",
        "paths": {
            "ui5/walkthrough/*": [
                "webapp/*"
            ]
        },
        "typeRoots": [
            "./webapp/types",
            "./node_modules/@types",
            "./node_modules/@sapui5"
        ]
    },
    "include": [
        "webapp/**/*"
    ]
}